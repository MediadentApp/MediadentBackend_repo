{
  "name": "studenhub_backend",
  "version": "1.0.0",
  "description": "Backend for StudenHub",
  "main": "./server.ts",
  "type": "module",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "dev": "nodemon --watch . --ext ts --exec tsx server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "test": "vitest --watch=false",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:bench": "vitest bench",
    "test:unit": "vitest unit",
    "test:integration": "vitest integration",
    "test:check-mongo-memory": "vitest run check-mongo-memory.test.ts",
    "test:algo": "vitest watch algo",
    "test:algo-memory": "npm run test:check-mongo-memory && npm run test:algo:watch"
  },
  "author": "Vinay Poojary",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.802.0",
    "@bull-board/api": "^6.9.6",
    "@bull-board/express": "^6.9.6",
    "aws4": "^1.13.2",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.52.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "mongoose-lean-virtuals": "^2.0.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "nodemailer": "^6.10.0",
    "socket.io": "^4.8.1",
    "ua-parser-js": "^2.0.3",
    "validator": "^13.15.0",
    "web-push": "^3.6.7"
  },
  "imports": {
    "#src/*": "./src/*"
  }
}